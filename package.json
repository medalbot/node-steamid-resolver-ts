{
  "name": "node-steamid-resolver-ts",
  "type": "module",
  "version": "2.0.2",
  "description": "Professional TypeScript library for Steam ID resolution with comprehensive error handling",
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://github.com/kWAYTV/node-steamid-resolver-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kWAYTV/node-steamid-resolver-ts.git"
  },
  "bugs": {
    "url": "https://github.com/kWAYTV/node-steamid-resolver-ts/issues"
  },
  "keywords": [
    "steam",
    "steamid",
    "typescript",
    "bun",
    "api",
    "gaming",
    "profile",
    "resolver"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist/**/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint --write",
    "lint:check": "biome lint",
    "format": "biome format --write",
    "format:check": "biome format",
    "prepublishOnly": "bun run clean && bun run build"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@biomejs/biome": "2.1.2",
    "@types/bun": "latest",
    "@types/xml2js": "^0.4.14",
    "bumpp": "^10.2.0",
    "tsup": "^8.0.0"
  }
}